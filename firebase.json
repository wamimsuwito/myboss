
{
  "hosting": {
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "mymanager",
          "region": "asia-east1"
        }
      }
    ],
    "i18n": {
      "root": "/"
    }
  }
}
